from queue import PriorityQueue 

def solution(scoville, K):
    answer = 0
    q = PriorityQueue()
    for ele in scoville:
        q.put(ele)
        
    while q.qsize() >= 2 :
        Min = q.get()
        if Min > K :
            break
        nextMin = q.get()
        q.put(Min+nextMin*2)
        answer += 1
        
    if q.get() < K :
        return -1
        
    return answer
